generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Calc {
  id        Int      @id @default(autoincrement())
  expr      String
  result    String
  updatedAt DateTime @updatedAt
  createAt  DateTime @default(now())
}
